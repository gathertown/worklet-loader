{
  "name": "@gathertown/worklet-loader",
  "version": "3.0.0",
  "author": "Gather Presence, Inc. <engineering@gather.town>",
  "description": "worklet loader module for webpack",
  "repository": {
    "url": "git+https://github.com/gathertown/worklet-loader.git"
  },
  "bugs": "https://github.com/gathertown/worklet-loader/issues",
  "homepage": "https://github.com/gathertown/worklet-loader",
  "license": "MIT",
  "main": "dist/cjs.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "yarn run build -w",
    "build": "cross-env NODE_ENV=production babel src -d dist --ignore 'src/**/*.test.js' --copy-files",
    "clean": "rm -rf dist",
    "lint": "eslint --cache src test",
    "lint-staged": "lint-staged",
    "prebuild": "yarn run clean",
    "prepare": "yarn run build",
    "release": "standard-version",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collectCoverageFrom='src/**/*.js' --coverage",
    "ci:lint": "yarn run lint",
    "ci:test": "yarn run test --runInBand",
    "ci:coverage": "yarn run test:coverage --runInBand",
    "defaults": "webpack-defaults",
    "webpack-defaults": "webpack-defaults"
  },
  "dependencies": {
    "loader-utils": "^2.0.4",
    "schema-utils": "^4.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@webpack-contrib/eslint-config-webpack": "~3.0.0",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-webpack": "^1.0.0",
    "eslint-plugin-import": "^2.2.0",
    "jest": "^29.7.0",
    "lint-staged": "^16.0.0",
    "pre-commit": "^1.0.0",
    "standard-version": "^9.5.0",
    "webpack": "^5.99.8"
  },
  "peerDependencies": {
    "@rspack/core": "0.x || 1.x",
    "webpack": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "@rspack/core": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.20.8,>= 20.19.2,>=22.15.1,>=24.0.2"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "packageManager": "yarn@4.9.1+sha512.f95ce356460e05be48d66401c1ae64ef84d163dd689964962c6888a9810865e39097a5e9de748876c2e0bf89b232d583c33982773e9903ae7a76257270986538"
}
